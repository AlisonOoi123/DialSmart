import pandas as pd

# Load your CSV
df = pd.read_csv('fyp_phoneDataset.csv')

# Check images for specific brands
brands_to_check = ['Apple', 'Asus', 'Google', 'Honor']
for brand in brands_to_check:
    brand_data = df[df['Brand'] == brand][['Brand', 'Model', 'ImageURL']]
    print(f"\n{brand}:")
    print(brand_data.head(3))
    
# Check for missing images
missing_images = df[df['ImageURL'].isna() | (df['ImageURL'] == '')]
print(f"\nTotal phones with missing images: {len(missing_images)}")
print(missing_images[['Brand', 'Model']].head(10))